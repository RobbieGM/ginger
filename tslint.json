{
  "extends": [
    "tslint-config-airbnb",
    "tslint-config-prettier",
    "tslint-plugin-prettier"
  ],
  "defaultSeverity": "warning",
  "rulesDirectory": ["tslint-plugin-prettier"],
  "rules": {
    "prettier": true,
    "import-name": false,
    "variable-name": [true, "allow-pascal-case"]
  }
}
